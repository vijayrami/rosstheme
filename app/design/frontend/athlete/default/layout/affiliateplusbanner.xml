<layout version="0.1.0">
    <affiliateplus_banner_list>
        <reference name="head">
            <action method="addJs"><script>tinybox/tinybox.js</script></action>
            <action method="addCss"><stylesheet>css/tinybox/style.css</stylesheet></action>
            <action method="addCss"><styleSheet>css/magestore/affiliateplusbanner.css</styleSheet></action>
        </reference>
        <remove name="affiliateplus_banner" />
        <reference name="content">
            <block type="affiliateplusbanner/banners" name="banner_list" template="affiliateplusbanner/banners.phtml" />
        </reference>
    </affiliateplus_banner_list>
</layout>
